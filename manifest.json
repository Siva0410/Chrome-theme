{
    "manifest_version": 2,
    "version": "1.0",
    "name": "futaba_theme",
    "description": "Futaba's chrome theme",
    "theme": {
        "images": {
            "theme_frame": "images/theme_frame.png",
            "theme_ntp_background": "images/futaba_wallpaper.png",
            "theme_tab_background": "images/theme_tab_background.png",
            "theme_toolbar": "images/theme_toolbar.png"
        },
        "colors": {
            "bookmark_text": [0, 0, 0],
            "button_background": [203, 203, 203, 0.9],
            "frame": [65, 117, 5],
            "ntp_background": [255, 255, 255],
            "ntp_text": [0, 0, 0],
            "tab_background_text": [64, 64, 64],
            "tab_text": [0, 0, 0],
            "toolbar": [223, 223, 223]
        },
        "tints": {
            "buttons": [65, 117, 5]
        },
        "properties": {
            "ntp_background_alignment": "center",
            "ntp_background_repeat": "no-repeat"
        }
    }
}